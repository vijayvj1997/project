pipeline{
    agent any
        stages{
            stage('clean'){
                steps{
                   echo 'Clean Stage'
                }
            }
            stage('compile'){
                            steps{
                                echo 'Compile Stage'
                                sh 'cd project\src\main\java'
                                sh 'javac main'
                            }
                        }
            stage('build'){
                            steps{
                                echo 'Build Stage'
                            }
                        }
            stage('test'){
                            steps{
                              echo 'Test Stage'
                            }
                        }
        }
}
